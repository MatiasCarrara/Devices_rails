<%= form_with scope: :device, url: devices_path, local: true do |form| %>  <div>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :address %><br>
    <%= form.number_field :address %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>
<%= link_to 'Back', devices_path %>
